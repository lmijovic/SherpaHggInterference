  Y_TITLE_OFFSET 1.4;
  X_TITLE_OFFSET 1.25;
  LEFT_MARGIN 0.14;
  BOTTOM_MARGIN 0.12;
  LEGEND_ENABLED NO;
  LINE_COLOUR 1;
  LINE_WIDTH 2;
}(devh);
